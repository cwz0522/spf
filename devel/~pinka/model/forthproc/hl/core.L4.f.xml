<def name="1+!" ds=" addr -- ">  1 SWAP +! </def>
<def name="1-!" ds=" addr -- "> -1 SWAP +! </def>

<def name="COUNT" ds=" b -- b +n "> DUP /CHAR + SWAP C@ </def>

